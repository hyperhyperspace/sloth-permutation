  "name": "@hyper-hyper-space/sloth-permutation",
  "description": "",
  "keywords": [],
  "version": "0.1.0",
  "homepage": "https://github.com/hyperhyperspace/sloth-permutation",
  "author": "Nazar Mokrynskyi <nazar@mokrynskyi.com>",
  "repository": {
    "type": "git",
